@model GigDetailsViewModel
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<partial name="_Navbar" for="NavbarModel" />

<h1>Gig Details</h1>

<div>
    <div class="form-group">
        <strong><label asp-for="Gig.ArtistName"></label></strong>
        <text class="form-control col-sm-6 gt-style-text-field">@Model.Gig.ArtistName</text>
    </div>

    <div class="form-group">
        <strong><label asp-for="Gig.VenueName"></label></strong>
        <text class="form-control col-sm-6 gt-style-text-field">@Model.Gig.VenueName</text>
    </div>

    <div class="form-group">
        <strong><label asp-for="Gig.VenueAddress"></label></strong>
        <text class="form-control col-sm-6 gt-style-text-field">@Model.Gig.VenueAddress</text>
    </div>

    <div class="form-group">
        <strong><label asp-for="Gig.VenuePhone"></label></strong>
        <text class="form-control col-sm-6 gt-style-text-field">@Model.Gig.VenuePhone</text>
    </div>

    <div class="form-group">
        <strong><label asp-for="Gig.Date"></label></strong>
        <text class="form-control col-sm-6 gt-style-text-field">@Model.Gig.Date</text>
    </div>
</div>


