@model GigDetailsViewModel
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Gig Details</h1>

<div class="form-group">
    <strong><label asp-for="Gig.ArtistName"></label></strong>
    <text class="form-control col-sm-6" >@Model.Gig.ArtistName</text>
</div>

<div class="form-group">
    <strong><label asp-for="Gig.VenueName"></label></strong>
    <text class="form-control col-sm-6" >@Model.Gig.VenueName</text>
</div>

<div class="form-group">
    <strong><label asp-for="Gig.VenueAddress"></label></strong>
    <text class="form-control col-sm-6" >@Model.Gig.VenueAddress</text>
</div>

<div class="form-group">
    <strong><label asp-for="Gig.VenuePhone"></label></strong>
    <text class="form-control col-sm-6" >@Model.Gig.VenuePhone</text>
</div>

<div class="form-group">
    <strong><label asp-for="Gig.Date"></label></strong>
    <text class="form-control col-sm-6" >@Model.Gig.Date</text>
</div>



